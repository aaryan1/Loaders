<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="CrosswordGrid">
		<attr name="squareSide" format="integer" />
	</declare-styleable>

	<declare-styleable name="ConcentricCircleView">
		<attr name="max_radius" format="integer" />
		<attr name="min_radius" format="integer" />
		<attr name="circles_count" format="integer" />
		<attr name="animate_color" format="boolean" />
		<attr name="duration" format="integer" />
		<attr name="start_color" format="color" />
		<attr name="end_color" format="color" />
	</declare-styleable>

	<declare-styleable name="HeartDrawView">
		<attr name="tValue" format="integer"/>
	</declare-styleable>

	<declare-styleable name="FillableLoader">
		<attr name="fl_fillColor"
			format="color"/>
		<attr name="fl_strokeColor"
			format="color"/>
		<attr name="fl_strokeWidth"
			format="dimension"/>
		<attr name="fl_originalWidth"
			format="integer"/>
		<attr name="fl_originalHeight"
			format="integer"/>
		<attr name="fl_strokeDrawingDuration"
			format="integer"/>
		<attr name="fl_fillDuration"
			format="integer"/>
		<attr name="fl_clippingTransform"
			format="enum">
			<enum name="plain"
				value="0"/>
			<enum name="spikes"
				value="1"/>
			<enum name="rounded"
				value="2"/>
			<enum name="waves"
				value="3"/>
			<enum name="squares"
				value="4"/>
			<enum name="bites"
				value="5"/>
		</attr>
	</declare-styleable>
</resources>